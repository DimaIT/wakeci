    "cypress": "^12.14.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.15.0",
    "javascript-time-ago": "^2.5.9",
    "lodash": "^4.17.21",
    "material-dynamic-colors": "^0.1.7",
    "minimist": "^1.2.8",
    "mitt": "^3.0.0",
    "prettier": "^2.8.8",
